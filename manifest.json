{
  "manifest_version": 3,
  "name": "RAVEN - Marketplace Finder",
  "version": "1.0.0",
  "description": "Find and track marketplace listings across Craigslist, Ebay, and Facebook Marketplace",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Open RAVEN"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
